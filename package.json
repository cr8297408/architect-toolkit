{
  "name": "architect-toolkit",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:esm": "tsc -p tsconfig.json --module esnext --outDir build/esm",
    "build:cjs": "tsc -p tsconfig.json --module commonjs --outDir build/cjs",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier:fix": "prettier --config .prettierrc --write .",
    "prettier": "prettier --config .prettierrc --check .",
    "flb": "npm run prettier:fix && npm run lint:fix && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sort-exports": "^0.8.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "semantic-release": "^22.0.7",
    "tslib": "~2.4",
    "tsutils": "~3.21",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "tslib": "^2.8.0"
  }
}
